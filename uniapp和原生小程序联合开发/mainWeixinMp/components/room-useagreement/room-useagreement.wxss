.prompt-mask{position:fixed;top:0;height:100%;width:100%;background-color:rgba(92,92,92,.5);z-index:999}.prompt-title{text-align:center;font-size:18px;padding-top:10px}.prompt-info{margin-top:10px;font-size:14px;margin:5%;float:left}.prompt-info-a{color:#34a853}.prompt-button-confirm{background-color:#34a853;border-radius:20rpx;height:70rpx;line-height:70rpx;font-size:28rpx;color:#fff;width:90%;margin-bottom:10px}.prompt-button-cancel{background-color:#fff;border-radius:20rpx;height:70rpx;line-height:70rpx;font-size:28rpx;color:#34a853;width:90%;border:1px solid}@media screen and (max-height:600px){.prompt{margin-top:20%;margin-left:5%;width:90%;height:77%;max-height:520px;background-color:#fff;border-radius:20rpx;overflow-y:auto}.prompt-footer{margin-bottom:10px}}@media screen and (min-height:601px){.prompt{margin-top:20%;margin-left:5%;width:90%;height:77%;max-height:520px;background-color:#fff;border-radius:20rpx;position:relative;overflow-y:auto}.prompt-footer{bottom:10px;position:absolute;width:100%}}