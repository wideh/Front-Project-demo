<view><view class="repair-info"><view class="card-list"><view class="list-item"><view class="title">预约日期</view><picker mode="date" start="{{startDate}}" data-event-opts="{{[['change',[['bindDateChange',['$event']]]]]}}" bindchange="__e"><view class="option"><view class="{{[appointment_date==''?'placholder-color':'']}}">{{appointment_date==''?'请选择（必选）':appointment_date}}</view><uni-icons vue-id="0dd9cd24-1" type="arrowright" bind:__l="__l"></uni-icons></view></picker></view><view class="list-item"><view class="title">预约时间</view><picker mode="time" data-event-opts="{{[['change',[['bindTimeChange',['$event']]]]]}}" bindchange="__e"><view class="option"><view class="{{[appointment_time==''?'placholder-color':'']}}">{{appointment_time==''?'请选择（必选）':appointment_time}}</view><uni-icons vue-id="0dd9cd24-2" type="arrowright" bind:__l="__l"></uni-icons></view></picker></view><view class="list-item"><view class="title">联系人</view><input class="item_input" placeholder="请输入联系人" data-event-opts="{{[['input',[['__set_model',['','applyUserName','$event',[]]]]]]}}" value="{{applyUserName}}" bindinput="__e"/></view><view class="list-item"><view class="title">联系电话</view><input class="item_input" placeholder="请输入联系电话" data-event-opts="{{[['input',[['__set_model',['','applyPhone','$event',[]]]]]]}}" value="{{applyPhone}}" bindinput="__e"/></view></view></view><view class="page-padding button-container"><button type="primary" data-event-opts="{{[['tap',[['submit',['$event']]]]]}}" bindtap="__e">提交</button></view></view>