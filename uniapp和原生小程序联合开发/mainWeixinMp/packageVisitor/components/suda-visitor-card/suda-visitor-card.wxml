<view class="{{[visitor.isCard?'suda-visitor-card suda-visitor-card-height':'suda-visitor-card']}}"><view class="{{['status',visitor.status?'used':'']}}">{{displayStatus}}</view><image class="star" src="https://pms-statics.oss-cn-shenzhen.aliyuncs.com/wechat-static/visitor/star.png"></image><view class="list-item"><view class="title small">您邀请了</view><view class="value large left">{{visitor.visitorName}}</view></view><view class="list-item"><view class="title small">到访</view><view class="value">{{visitor.houseAddress?visitor.houseAddress:'-'}}</view></view><view class="hr"></view><view class="list-item"><view class="title">手机号码</view><view class="value">{{visitor.visitorPhone?visitor.visitorPhone:'-'}}</view></view><view class="list-item"><view class="title">来访人数</view><view class="value">{{visitor.visitorNum?visitor.visitorNum+'人':'-'}}</view></view><view class="list-item"><view class="title">来访事由</view><view class="value">{{visitor.visitorReason?visitor.visitorReason:'-'}}</view></view><view class="list-item"><view class="title">有效时间</view><view class="value">{{displayTime}}</view></view><view class="list-item"><view class="title">开门密码</view><view class="value">{{visitor.password||'-'}}</view></view><view class="list-item"><view class="title">通行二维码</view><view class="value"><image class="code" src="{{visitor.qrCode}}" data-event-opts="{{[['tap',[['previewClicked',['$0'],['visitor.qrCode']]]]]}}" bindtap="__e"></image></view></view><view class="bottom">-此卡片当日有效-</view></view>