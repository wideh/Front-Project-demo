<view><view class="info"><view data-event-opts="{{[['tap',[['open',['$event']]]]]}}" class="list-item account-container" bindtap="__e"><view class="title">预缴账户</view><view class="fnAccountName-container"><block wx:if="{{$root.g0}}"><image class="img" src="{{$root.m0}}"></image></block><text class="content fn-account-name">{{$root.g1>0?chargeInfo.fnAccountName||'请选择账户':'无账户'}}</text><block wx:if="{{$root.g2>0}}"><uni-icons class="arrow" vue-id="625f0ad5-1" type="arrowright" color="#C7C7CC" size="18" bind:__l="__l"></uni-icons></block></view></view><view class="list-item balance-container"><view class="title">余额</view><view class="content balance">{{balance}}</view></view><block wx:if="{{!isContactBill}}"><view data-event-opts="{{[['tap',[['goToSelectResource',['$event']]]]]}}" class="list-item" bindtap="__e"><view class="title">房间号</view><view class="options"><view class="content">{{resourceAddress||(canChangeResource?'请选择其他房间':'')}}</view><block wx:if="{{canChangeResource}}"><uni-icons class="arrow" vue-id="625f0ad5-2" type="arrowright" color="#C7C7CC" size="18" bind:__l="__l"></uni-icons></block></view></view></block></view><view class="select-amount"><block wx:if="{{$root.g3>0}}"><block><view class="title">充值金额</view><view class="select-options"><block wx:for="{{chargeInfo.recommendedAmountSetting.recommendedAmounts}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['selectAmount',[index]]]]]}}" class="{{['option',amountIndex==index?'selected':'']}}" bindtap="__e">{{''+item+'元'}}</view></block></view><view class="{{['amountinput-view',isFocus?'selected':'']}}"><input class="amount-input" type="digit" placeholder="{{chargeInfo.minAmount!=null?'自定义金额(不少于'+chargeInfo.minAmount+')':'自定义金额'}}" placeholder-style="font-size: 8pt; color:#d8d8d8;" data-event-opts="{{[['input',[['onKey_Input',['$event']]]],['focus',[['changeFocus',[true]]]],['blur',[['changeFocus',[false]]]]]}}" value="{{inputAmount}}" bindinput="__e" bindfocus="__e" bindblur="__e"/><label class="amount-label">元</label></view><view class="button-container"><button type="primary" data-event-opts="{{[['tap',[['recharge',['$event']]]]]}}" bindtap="__e">立即充值</button><button class="charge-log-btn" type="default" data-event-opts="{{[['tap',[['goToChargeLog',['$event']]]]]}}" bindtap="__e">充值记录</button></view></block></block><block wx:else><view><text class="errText">{{errText}}</text></view></block></view><view class="bottom-container"><custom-footer vue-id="625f0ad5-3" data="{{footerData}}" bind:__l="__l"></custom-footer></view><suda-popup class="vue-ref" vue-id="625f0ad5-4" data-ref="select-account" bind:__l="__l" vue-slots="{{['default']}}"><view data-event-opts="{{[['tap',[['cancel',['$event']]]]]}}" class="close" bindtap="__e"><uni-icons vue-id="{{('625f0ad5-5')+','+('625f0ad5-4')}}" size="28" type="closeempty" bind:__l="__l"></uni-icons></view><view class="select-account-title"><text>选择预缴账户</text></view><block wx:if="{{$root.g4>0}}"><block><block wx:for="{{$root.l0}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['selectAccount',['$0'],[[['accounts','',index]]]]]]]}}" class="item" bindtap="__e"><image class="img" src="{{item.m1}}"></image><text class="name">{{item.$orig.name}}</text><view class="amount">{{"余额 ¥ "+item.$orig.amount}}</view><view><uni-icons vue-id="{{('625f0ad5-6-'+index)+','+('625f0ad5-4')}}" type="arrowright" bind:__l="__l"></uni-icons></view></view></block></block></block><block wx:else><view class="no-accounts"><text>暂无预缴账户</text></view></block></suda-popup></view>