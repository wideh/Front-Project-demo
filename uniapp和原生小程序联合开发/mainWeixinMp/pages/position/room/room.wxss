.prompt-mask{position:fixed;top:0;height:100%;width:100%;background-color:rgba(92,92,92,.5);z-index:999}.prompt-title{text-align:center;font-size:18px;padding-top:10px}.prompt-info{margin-top:10px;font-size:14px;margin:5%;float:left}.prompt-info-a{color:#34a853}.prompt-button-confirm{background-color:#34a853;border-radius:20rpx;height:70rpx;line-height:70rpx;font-size:28rpx;color:#fff;width:90%;margin-bottom:10px}.prompt-button-cancel{background-color:#fff;border-radius:20rpx;height:70rpx;line-height:70rpx;font-size:28rpx;color:#34a853;width:90%;border:1px solid}@media screen and (max-height:600px){.prompt{margin-top:20%;margin-left:5%;width:90%;height:77%;max-height:520px;background-color:#fff;border-radius:20rpx;overflow-y:auto}.prompt-footer{margin-bottom:10px}}@media screen and (min-height:601px){.prompt{margin-top:20%;margin-left:5%;width:90%;height:77%;max-height:520px;background-color:#fff;border-radius:20rpx;position:relative;overflow-y:auto}.prompt-footer{bottom:10px;position:absolute;width:100%}}.position-room .select-all{position:fixed;top:0;left:0;right:0;height:80rpx;padding:0 40rpx;display:flex;justify-content:flex-end;align-items:center;color:#34a853;z-index:10;background:hsla(0,0%,95.3%,.5)}.position-room .list-wrapper{margin-top:80rpx}.footer-padding{padding-bottom:calc(var(--safe-area-inset-bottom) + 60rpx + 94rpx)}.position-room-list{display:flex;flex-wrap:wrap;justify-content:center}.position-room-list .suda-room-card{margin-bottom:40rpx}.position-room-no-phone-wrapper{font-size:24rpx;color:#666;margin:0 40rpx;display:flex;align-items:center}.position-room-change-phone-btn{display:inline-block;background:none;border:none;padding:0;line-height:1;outline:none;font-size:24rpx;border-radius:0;color:#34a853}.position-room-change-phone-btn::after{border:none}.position-room .bottom-button-container{background-color:#f6f4f4}